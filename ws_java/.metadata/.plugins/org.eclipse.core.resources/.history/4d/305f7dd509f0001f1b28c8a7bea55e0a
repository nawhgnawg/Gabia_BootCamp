package oop;

public class APITest {

  public static void main(String[] args) {
    String seanson1 = "Spring";
    String seanson2 = new String("Spring");   // 권장하지 않음.
    
    System.out.println(seanson1);
    System.out.println(seanson2);
    
    System.out.println(seanson1.toUpperCase());
    System.out.println(seanson2.toUpperCase());
  }

}
