package oop;

public class SplitTest {
  
  public static void main(String[] args) {
    String season = "봄,여름,가을,겨울";
    String[] split = season.split(",");
    String[] list = split;
    
    season.split(",");
    for (String item : list) {
      System.out.print(item + " ");
    }
   
  }
  
}
