package tool;

public class Tool {
  
  public static String comma(int num) {
    return  String.format("%,d", num);
  }
  
  public static boolean isImage(String fileName) {
    if (fileName.endsWith("jpg") || fileName.endsWith("gif") || fileName.endsWith("png")) {
      return true;
    }
    
    return false;
  }

}
