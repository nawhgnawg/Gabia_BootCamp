package jcf;

public class MovieVO {
  
  private String title;
  private double score;
  private String actor;
  
  
  public MovieVO() {
    
  }
  
  public MovieVO(String title, double score, String actor) {
    this.title = title;
    this.score = score;
    this.actor = actor;
  }

  public String getTitle() {
    return title;
  }

  public double getScore() {
    return score;
  }

  public String getActor() {
    return actor;
  }
  

}
